  "tabBar": {
    "color": "@tabFontColor",
    "selectedColor": "@tabSelectedColor",
    "backgroundColor": "@tabBgColor",
    "borderStyle": "@tabBorderStyle",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "18px",
    "spacing": "3px",
    // "iconfontSrc": "static/iconfont.ttf", // app tabbar 字体.ttf文件路径 app 3.4.4+
    "list": [
      {
        "pagePath": "pages/start/index",
        "iconPath": "@startIconPath",
        "selectedIconPath": "@startSelectedIconPath",
        "text": "开始"
      },
      {
        "pagePath": "pages/component/index",
        "iconPath": "@componentsIconPath",
        "selectedIconPath": "@componentsSelectedIconPath",
        "text": "内置组件"
      },
      {
        "pagePath": "pages/uni-api/index",
        "iconPath": "@apiIconPath",
        "selectedIconPath": "@apiSelectedIconPath",
        "text": "API"
      },
      {
        "pagePath": "pages/uni-ui/index",
        "iconPath": "@uiIconPath",
        "selectedIconPath": "@uiSelectedIconPath",
        "text": "扩展组件"
      }
    ]
    // "midButton": {
